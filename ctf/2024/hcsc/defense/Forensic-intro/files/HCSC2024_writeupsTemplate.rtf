        mutations.forEach(function(mutation) {
            mutation.addedNodes.forEach(function(node) {
              
                if (node.nodeType === Node.ELEMENT_NODE && node.classList.contains('challenge-name')) {
                    if (node.textContent.trim() === "Creeper or griefer?") {
                        node.classList.add('special-style');
                    }
                }

                
                node.querySelectorAll && node.querySelectorAll('h2.challenge-name').forEach(function(h2) {
                    if (h2.textContent.trim() === "Creeper or griefer?") {
                        h2.classList.add('special-style');
                    }
                });
            });
        });
    });

    var config = {
        childList: true,
        subtree: true
    };

    observer.observe(targetNode, config);

});
  
